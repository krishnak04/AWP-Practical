# Create an application to demonstrate following operations. #
# i. Generate Fibonacci series. #

using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Practical_1C
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int num1 = 0, num2 = 1, num3, i, number;
            Console.WriteLine("Name: krishna.k");
            Console.WriteLine("How many numbers do you want to print:---?");
            number = int.Parse(Console.ReadLine());
            Console.WriteLine($"Result:---");
            Console.Write(num1 + "\t" + num2);
            for (i = 2; i < number; ++i)
            {
                num3 = num1 + num2;
                Console.Write("\t" + num3);
                num1 = num2;
                num2 = num3;
            }
        }
    }
}
